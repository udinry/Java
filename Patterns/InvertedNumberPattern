import java.util.Scanner;
public class Solution {


	public static void main(String[] args) {
	
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
      	int temp=n;
		for(int i=1;i<=n;i++)
		{
//			for(int k=1;k<=n-i;k++)
//			{
//				System.out.print(" ");
//			}
			for(int j=1;j<=n-i+1;j++)
			{
				System.out.print(temp);
              
			}
			System.out.println();
          temp--;
		}

		
	}

}
