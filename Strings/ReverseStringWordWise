public class Solution {
	public static String reverseWordWise(String input) {
		// Write your code here
//int i;
      int j;
     
      String lal="";
      String s[]=input.split(" ");
      for(int i=s.length-1;i>=0;i--)
      {
        lal+=s[i]+" ";
      }
      return(lal.substring(0,lal.length()-1)); 
	}
}
